<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
    <style>
        th,
        td {
            padding: 15px;
            text-align: left;
        }

        tr:nth-child(even) {
            background-color: #f2f2f2;
        }

        th {
            background-color: #4CAF50;
            color: white;
        }
    </style>
</head>

<body>
    <h1>This is list of students</h1>

    <table>
        <tr>
            <th>First Name</th>
            <th>Last Name</th>
            <th>Email</th>
            <th>Gender</th>
            <th colspan="3">Birth Date</th>
        </tr>
        <% students_list.forEach(item => { %>
        <tr>
            <td> <%= item.first_name %> </td>
            <td> <%= item.last_name %> </td>
            <td> <%= item.email %> </td>
            <td> <%= item.gender %> </td>
            <td> <%= item.birth_date %> </td>
            <td><a href="/students/<%= item.id %>/edit">Edit</a></td>
            <td><a href="/students/<%= item.id %>/delete">Delete</a></td>
        </tr>
        <% }) %>
    </table>

</body>

</html>